<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- is.prodstar.fp2art -->
        <record id="is_prodstar_fp2art_tree_view" model="ir.ui.view">
            <field name="name">is.prodstar.fp2art.tree</field>
            <field name="model">is.prodstar.fp2art</field>
            <field name="arch" type="xml">
                <tree string="Prodstar FP2ART" default_order="code_pg">
                    <field name="soc"            optional="show"/>
                    <field name="code_pg"        optional="show"/>
                    <field name="designation"    optional="show"/>
                    <field name="cat"            optional="show"/>
                    <field name="fam"            optional="show"/>
                    <field name="gest"           optional="show"/>
                    <field name="moule"          optional="show"/>
                    <field name="projet"         optional="show"/>
                    <field name="ref_plan"       optional="hide"/>
                    <field name="ind"            optional="hide"/>
                    <field name="us"             optional="show"/>
                    <field name="uc"             optional="show"/>
                    <field name="uc_us"          optional="hide"/>
                    <field name="designation_uc" optional="hide"/>
                    <field name="um_uc"          optional="hide"/>
                    <field name="sr"             optional="hide"/>
                </tree>
            </field>
        </record>
        <record id="is_prodstar_fp2art_form_view" model="ir.ui.view">
            <field name="name">is.prodstar.fp2art.form</field>
            <field name="model">is.prodstar.fp2art</field>
            <field name="arch" type="xml">
                <form string="Prodstar FP2ART">
                    <sheet>
                        <group>
                            <group string="Informations générales">
                                <field name="soc"/>
                                <field name="code_pg"/>
                                <field name="designation"/>
                                <field name="cat"/>
                                <field name="fam"/>
                                <field name="gest"/>
                                <field name="moule"/>
                                <field name="projet"/>
                            </group>
                            <group string="Spécifications techniques">
                                <field name="ref_plan"/>
                                <field name="ind"/>
                                <field name="us"/>
                                <field name="uc"/>
                                <field name="uc_us"/>
                                <field name="designation_uc"/>
                                <field name="um_uc"/>
                                <field name="sr"/>
                            </group>
                        </group>
                        <group string="Spécifications internes">
                            <field name="specif_interne" nolabel="1"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="is_prodstar_fp2art_search_view" model="ir.ui.view">
            <field name="name">is.prodstar.fp2art.search</field>
            <field name="model">is.prodstar.fp2art</field>
            <field name="arch" type="xml">
                <search>
                    <field name="code_pg" string="Code PG" filter_domain="[('code_pg','ilike',self)]"/>
                    <field name="designation" string="Désignation" filter_domain="[('designation','ilike',self)]"/>
                    <field name="moule" string="Moule" filter_domain="[('moule','ilike',self)]"/>
                    <field name="projet" string="Projet" filter_domain="[('projet','ilike',self)]"/>
                    <field name="gest" string="Gestionnaire" filter_domain="[('gest','ilike',self)]"/>
                    <group expand="0" string="Grouper par">
                        <filter name="group_soc" string="Société" context="{'group_by':'soc'}"/>
                        <filter name="group_cat" string="Catégorie" context="{'group_by':'cat'}"/>
                        <filter name="group_fam" string="Famille" context="{'group_by':'fam'}"/>
                        <filter name="group_gest" string="Gestionnaire" context="{'group_by':'gest'}"/>
                        <filter name="group_moule" string="Moule" context="{'group_by':'moule'}"/>
                        <filter name="group_projet" string="Projet" context="{'group_by':'projet'}"/>
                    </group>
                </search>
            </field>
        </record>
        <record id="is_prodstar_fp2art_action" model="ir.actions.act_window">
            <field name="name">Articles Prodstar</field>
            <field name="res_model">is.prodstar.fp2art</field>
            <field name="view_mode">tree,form</field>
        </record>

